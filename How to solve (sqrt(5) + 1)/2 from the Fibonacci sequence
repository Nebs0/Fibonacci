import math

def calculate_phi():
    n = 1000  # Use a large value of n to get an accurate approximation
    phi = (math.sqrt(5) + 1) / 2
    fibonacci_numerator = phi**n - (1 - phi)**n
    fibonacci_denominator = math.sqrt(5)
    result = fibonacci_numerator / fibonacci_denominator
    return result

golden_ratio_approximation = calculate_phi()
print(golden_ratio_approximation)
